{
  "name": "apisix_api",
  "description": "APISIX API 插件",
  "version": "1.0.0",
  "author": "baiuu",
  "config": [
    {
      "name": "admin_key",
      "type": "string",
      "description": "AdminKey",
      "required": true
    },
    {
      "name": "server_address",
      "type": "string",
      "description": "服务地址",
      "required": true
    }
  ],
  "actions": [
    {
      "name": "upload_bind",
      "description": "上传绑定",
      "params": [
        {
          "name": "cert",
          "type": "string",
          "description": "证书内容",
          "required": true
        },
        {
          "name": "key",
          "type": "string",
          "description": "私钥内容",
          "required": true
        },
        {
          "name": "domain",
          "type": "array",
          "description": "域名列表",
          "required": true
        }
      ]
    }
  ]
}
